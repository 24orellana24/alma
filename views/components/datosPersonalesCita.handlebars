<form method="post" action="/dashboard/datos-personales" enctype="multipart/form-data">
  <div class="form-group row w-80 m-auto">
    <div class="form-group col-12 col-sm-6 mb-3">
      <label>Rut</label>
      <input id="rut" name="rut" class="form-control m-auto text-center" value="{{cliente.rut}}" readonly/>
    </div>
    <div class="form-group col-12 col-sm-6 mb-3">
      <label>Email</label>
      <input id="email" name="email" class="form-control m-auto text-center" value="{{cliente.email}}" readonly/>
    </div>
    <div class="form-group col-12 col-sm-6 mb-3">
      <label>Apellido Paterno</label>
      <input id="apellido_paterno" name="apellido_paterno" class="form-control m-auto text-center" value="{{cliente.apellidopaterno}}" readonly/>
    </div>
    <div class="form-group col-12 col-sm-6 mb-3">
      <label>Apellido Materno</label>
      <input id="apellido_materno" name="apellido_materno" class="form-control m-auto text-center" value="{{cliente.apellidomaterno}}" readonly/>
    </div>
    <div class="form-group col-12 col-sm-6 mb-3">
      <label>Nombres</label>
      <input id="nombre" name="nombre" class="form-control m-auto text-center" value="{{cliente.nombre}}" readonly/>
    </div>
    <div class="form-group col-12 col-sm-6 mb-3">
      <label>Fecha Nacimiento</label>
      <input id="fecha_nacimiento" name="fecha_nacimiento" type="date" value="{{cliente.fechanacimiento}}" class="form-control m-auto text-center" readonly/>
    </div>
    <div class="form-group col-12 col-sm-6 mb-3">
      <label>NÃºmero de Celular</label>
      <input id="celular" name="celular" class="form-control m-auto text-center" value="{{cliente.celular}}" readonly/>
    </div>
    <div class="form-group col-12 col-sm-6 mb-3">
      <label>Comuna de Residencia</label>
      <input id="comuna" name="comuna" class="form-control m-auto text-center" value="{{cliente.comuna}}" readonly/>
    </div>
  </div>
